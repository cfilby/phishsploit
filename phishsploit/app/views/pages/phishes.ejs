<% include ../template/head %>

<div class="row">
    <div class="card">
      <div class="card-content">
        <span class="card-title grey-text text-darken-4"><%= title %> (<%= total.count %>)</span>

        <div class="collection">
        <% phishes.forEach(function(phish) { %>
          <a href="/phish/specific/<%= phish.phishId %>" class="collection-item truncate"><%= phish.fullUrl %></a>
        <% }); %>
        </div>

        <% include ../partials/pagination %>
      </div>
    </div>
</div>

<% include ../template/foot %>
